{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.5.9",
  "manifest_version": 2,
  "minimum_chrome_version": "41",

  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",

  "offline_enabled": true,
  "default_locale": "en",

 

  "background": {
    "scripts": [
    "js/background_script_binary.js",
    "js/libraries.js",
    "js/background.js"
    ],
    "persistent": true
  },

  "permissions": [
    "https://*/*",
    "storage",
    "enterprise.deviceAttributes",
    "enterprise.platformKeys",
    "clipboardWrite",
    "alarms",
    "notifications",
    "identity",
    "identity.email",
    "platformKeys",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "cookies"
  ],

  "storage": {
    "managed_schema": "managed_policy_schema.json"
  },

  "browser_action": {
    "default_icon": {
      "19": "img/security_googblue_19dp.png",
      "38": "img/security_googblue_38dp.png"
    }
  },

  "icons": {
    "16": "img/security_googblue_16dp.png",
    "48": "img/security_googblue_48dp.png",
    "128": "img/security_googblue_128dp.png"
  }
}
